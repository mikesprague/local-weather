{
  "name": "local-weather",
  "version": "0.19.6",
  "description": "Minimalist local weather app powered by Dark Sky",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "yarn webpack",
    "deploy": "now --public",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn dev:serve",
    "webpack": "webpack --config ./webpack.config.js --progress --colors",
    "clean:public": "rm -rf ./public > /dev/null 2>&1",
    "dev:build": "yarn clean:public && yarn dev:webpack",
    "dev:now": "cd public; now --public; cd ../",
    "dev:deploy": "yarn dev:now && yarn clean:public",
    "dev:serve": "webpack-serve ./webpack.config.dev.js --port 5500 --open",
    "dev:webpack": "webpack --config ./webpack.config.dev.js --progress --colors",
    "sass:compile": "node-sass src/scss -o public/css --output-style compressed",
    "sass:lint": "sass-lint --verbose --no-exit",
    "sass:watch": "node-sass --watch src/scss -o public/css --output-style compressed"
  },
  "repository": {
    "url": "https://github.com/mikesprague/local-weather"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "GNU GPLv3",
  "bugs": {
    "url": "https://github.com/mikesprague/local-weather/issues"
  },
  "homepage": "https://localweather.io",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/free-regular-svg-icons": "^5.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "bootstrap": "^4.1.3",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "eslint-plugin-prettier": "^2.6.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "prettier": "^1.14.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "sweetalert2": "^7.26.10",
    "tippy.js": "^2.5.4",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "webpack-serve": "^2.0.2"
  }
}
