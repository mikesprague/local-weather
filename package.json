{
  "name": "local-weather",
  "version": "0.16.0",
  "description": "Minimalist local weather app powered by Dark Sky",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn webpack:serve",
    "webpack": "webpack --progress --colors",
    "webpack:dev": "yarn webpack --mode development --watch",
    "webpack:build": "yarn webpack --mode production",
    "webpack:serve": "webpack-serve ./webpack.config.js --port 5500 --open",
    "sass:watch": "node-sass --watch src/scss -o dist/css --output-style compressed",
    "sass:compile": "node-sass src/scss -o dist/css --output-style compressed",
    "sass:lint": "sass-lint --verbose --no-exit"
  },
  "repository": {
    "url": "https://github.com/mikesprague/local-weather"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "GNU GPLv3",
  "bugs": {
    "url": "https://github.com/mikesprague/local-weather/issues"
  },
  "homepage": "https://localweather.io",
  "dependencies": {},
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/pro-light-svg-icons": "^5.2.0",
    "@fortawesome/pro-regular-svg-icons": "^5.2.0",
    "@fortawesome/pro-solid-svg-icons": "^5.2.0",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "sweetalert2": "^7.26.9",
    "tippy.js": "^2.5.4",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  }
}
