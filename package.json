{
  "name": "local-weather",
  "version": "0.44.1",
  "description": "Minimalist local weather app powered by Dark Sky",
  "private": true,
  "main": "./public/index.html",
  "scripts": {
    "build": "yarn clean && yarn webpack && yarn lambda:build",
    "clean": "rm -rf ./public > /dev/null 2>&1",
    "serve": "concurrently --kill-others --raw 'yarn webpack:serve' 'yarn lambda:serve'",
    "start": "yarn serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --config ./webpack.config.js --progress --colors",
    "webpack:dev": "webpack --config ./webpack.config.dev.js --progress --colors",
    "webpack:serve": "webpack-serve ./webpack.config.js --port 5500 --open",
    "webpack:serve:dev": "webpack-serve ./webpack.config.dev.js --port 5500 --open",
    "dev:build": "yarn dev:clean && yarn webpack:dev && yarn lambda:build",
    "dev:clean": "rm -rf ./build > /dev/null 2>&1",
    "dev:serve": "concurrently --kill-others --raw 'yarn webpack:serve:dev' 'yarn lambda:serve'",
    "lambda:build": "netlify-lambda build ./src/functions",
    "lambda:serve": "netlify-lambda serve ./src/functions -c ./lambda.config.dev.js"
  },
  "repository": {
    "url": "https://github.com/mikesprague/local-weather"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesprague/local-weather/issues"
  },
  "homepage": "https://localweather.io",
  "dependencies": {},
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.9",
    "@fortawesome/free-brands-svg-icons": "^5.6.0",
    "@fortawesome/pro-light-svg-icons": "^5.6.0",
    "@fortawesome/pro-regular-svg-icons": "^5.6.0",
    "@fortawesome/pro-solid-svg-icons": "^5.6.0",
    "bugsnag": "^2.4.3",
    "bulma": "^0.7.2",
    "compression-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "dayjs": "^1.7.7",
    "dotenv": "^6.2.0",
    "eslint": "^5.10.0",
    "eslint-plugin-prettier": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "netlify-cli": "^2.5.0",
    "netlify-lambda": "^1.1.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.3",
    "request": "^2.34.0",
    "request-promise": "^4.2.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "sweetalert2": "^7.32.2",
    "tippy.js": "^3.3.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.27.1",
    "webpack-bugsnag-plugins": "^1.2.3",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.3"
  }
}
