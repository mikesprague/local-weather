  },
  "homepage": "https://localweather.io",
  "dependencies": {},
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.1.0-4",
    "bootstrap": "^4.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.9.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "sweetalert2": "^7.24.4",
