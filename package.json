{
  "name": "local-weather",
  "version": "0.28.1",
  "description": "Minimalist local weather app powered by Dark Sky",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "yarn clean && yarn webpack",
    "clean": "rm -rf ./public > /dev/null 2>&1",
    "deploy": "yarn push && yarn clean",
    "push": "netlify deploy --prod --dir public --site 91ecd22f-e283-48e1-ab1a-925a57b8dc42",
    "start": "yarn dev:serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --config ./webpack.config.js --progress --colors",
    "dev:api": "docker run --env-file=../api-localweather-io/.env --rm -p 3000:3000 api-localweather-io:latest",
    "dev:build": "yarn dev:clean && yarn dev:webpack",
    "dev:clean": "rm -rf ./build > /dev/null 2>&1",
    "dev:deploy": "yarn dev:push && yarn dev:clean",
    "dev:push": "netlify deploy --prod --dir build --site 263c7c4b-2903-48a2-9fd7-25aefbf0f757",
    "dev:serve": "concurrently --kill-others --raw 'webpack-serve ./webpack.config.dev.js --port 5500 --open' 'yarn dev:api'",
    "dev:watch": "yarn dev:clean && yarn dev:webpack --watch",
    "dev:webpack": "webpack --config ./webpack.config.dev.js --progress --colors"
  },
  "repository": {
    "url": "https://github.com/mikesprague/local-weather"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "GNU GPLv3",
  "bugs": {
    "url": "https://github.com/mikesprague/local-weather/issues"
  },
  "homepage": "https://localweather.io",
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/pro-light-svg-icons": "^5.5.0",
    "@fortawesome/pro-regular-svg-icons": "^5.5.0",
    "@fortawesome/pro-solid-svg-icons": "^5.5.0",
    "bulma": "^0.7.2",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-prettier": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "sweetalert2": "^7.29.0",
    "tippy.js": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.25.1",
    "webpack-bugsnag-plugins": "^1.2.2",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "webpack-serve": "^2.0.2"
  }
}
